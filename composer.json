{
    "name": "sxbrsky/splx",
    "description": "An extension to the Standard PHP Library (SPL).",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "set",
        "map",
        "vector",
        "spl"
    ],
    "authors": [
        {
            "name": "Dominik Szamburski",
            "email": "dominikszamburski99@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/sxbrsky/php-splx/issues",
        "source": "https://github.com/sxbrsky/php-splx"
    },
    "require": {
        "php": ">=8.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "dev-master",
        "mockery/mockery": "2.0.x-dev",
        "phpstan/phpstan": "2.0.x-dev",
        "phpunit/phpunit": "11.4.x-dev"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Splx\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Splx\\Tests\\": "tests/Tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
